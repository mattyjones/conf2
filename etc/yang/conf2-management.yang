module conf2-management {
	namespace "http://org.conf2/ns/management";
	prefix "conf2";
	revision 0;

	grouping management {
        leaf port {
            type string;
        }
        leaf docRoot {
            type string;
        }
        leaf path {
            type string;
            default "/restconf/";
        }
        leaf-list registrations {
            type string;
            config "false";
        }
	}
}
